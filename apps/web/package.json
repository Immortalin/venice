{
  "name": "@usevenice/web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "pnpm run generate:css --minify",
    "build:migration": "if [ \"$VERCEL_ENV\" = production ]; then pnpm --dir ../../ migration up; else echo 'Skip non-production migration'; fi",
    "build:next": "next build",
    "build:worker": "if [ \"$VERCEL_ENV\" = production ]; then pnpm --dir ../../ worker:setup; else echo 'Skip non-production worker setup'; fi",
    "dev": "run-p --silent dev:*",
    "dev:css": "pnpm run generate:css --watch",
    "dev:next": "next dev --hostname 127.0.0.1",
    "generate:css": "tailwindcss -i ./global.css -o ./__generated__/tailwind.css",
    "start": "next start"
  },
  "dependencies": {
    "@trpc/server": "9.26.0",
    "@usevenice/app-config": "workspace:*",
    "@usevenice/cdk-core": "workspace:*",
    "@usevenice/engine-backend": "workspace:*",
    "@usevenice/engine-frontend": "workspace:*",
    "@usevenice/ui": "workspace:*",
    "@usevenice/util": "workspace:*",
    "@usevenice/worker": "workspace:*",
    "cookies-next": "2.1.1",
    "jotai": "1.8.3",
    "next": "12.2.5",
    "next-query-params": "4.0.0",
    "next-router-query": "0.1.3",
    "phosphor-react": "1.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "3.39.2",
    "superjson": "1.9.1",
    "tailwind-merge": "1.6.0",
    "ts-pattern": "4.0.5",
    "use-query-params": "2.1.0"
  },
  "devDependencies": {
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "autoprefixer": "*",
    "csstype": "*",
    "daisyui": "2.24.2",
    "next-transpile-modules": "9.0.0",
    "node-loader": "2.0.0",
    "postcss": "*",
    "tailwindcss": "3.1.8",
    "tailwindcss-radix": "2.5.0",
    "tilg": "0.1.1",
    "webpack": "*"
  }
}
